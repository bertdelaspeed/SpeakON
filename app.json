{
  "expo": {
    "name": "SpeakON",
    "slug": "speakon",
    "scheme": "com.bertdelaspeed.speakon",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/LetsChat_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/LetsChat_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.bertdelaspeed.speakon",
      "adaptiveIcon": {
        "foregroundImage": "./assets/LetsChat_splash.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "997ffffc-18f3-4ed2-ab7d-448e5574269e"
      }
    },
    "owner": "bertdelaspeed "
  }
}
